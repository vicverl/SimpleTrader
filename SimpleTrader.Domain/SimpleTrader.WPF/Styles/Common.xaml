<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--#region Colors-->
    <Color x:Key="PrimaryColor1">#799540</Color>
    <Color x:Key="PrimaryColor2">#50632b</Color>
    <!--#endregion-->

    <!--#region Brushes-->
    <SolidColorBrush x:Key="BrushPrimary1" Color="{StaticResource PrimaryColor1}" />
    <SolidColorBrush x:Key="BrushPrimary2" Color="{StaticResource PrimaryColor2}" />
    <!--#endregion-->
</ResourceDictionary>